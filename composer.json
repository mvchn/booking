    "symfony/asset": "5.x-dev",
    "symfony/monolog-bundle": "3.x-dev",
    "symfony/mailer": "5.x-dev",
    "symfony/form": "5.x-dev",
    "symfony/security-bundle": "5.x-dev",
    "symfony/translation": "5.x-dev",
    "symfony/validator": "5.x-dev",
    "composer/package-versions-deprecated": "^1.0@dev",
    "doctrine/doctrine-bundle": "2.4.x-dev",
